; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="DragonWarrior"
run/main_scene="res://ui/title_screen/title_screen.tscn"
config/features=PackedStringArray("4.2", "Mobile")
config/icon="res://icon.svg"

[autoload]

FileUtils="*res://singleton/file_utils.gd"
MenuStack="*res://singleton/menu_stack.gd"
GeneralDialogueProvider="*res://singleton/general_dialogue_provider.gd"
DebugUtils="*res://singleton/debug_utils.tscn"
PlayerManager="*res://singleton/player_manager/player_manager.tscn"
GlobalVisuals="*res://singleton/global_visuals/global_visuals.tscn"
AudioManager="*res://singleton/audio_manager/audio_manager.tscn"
GameDataManager="*res://singleton/game_data_manager/game_data_manager.tscn"
GameSettings="*res://singleton/settings/game_settings.tscn"
DebugConsole="*res://singleton/debug_console/debug_console.tscn"

[display]

window/size/viewport_width=256
window/size/viewport_height=224
window/size/window_width_override=1024
window/size/window_height_override=896
window/stretch/mode="canvas_items"

[editor]

export/convert_text_resources_to_binary=false

[importer_defaults]

font_data_image={
"character_margin": Rect2i(0, 0, 0, 0),
"character_ranges": PackedStringArray("\'A\'-\'Z\'", "\'0\'-\'9\'", "\'!\'", "\'.\'", "45", "\'>\'", "\'c\'", "\' \'"),
"columns": 14,
"compress": true,
"fallbacks": [],
"image_margin": Rect2i(0, 0, 0, 0),
"rows": 3,
"scaling_mode": 2
}

